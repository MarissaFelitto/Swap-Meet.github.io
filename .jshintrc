{
  "globals": {
    "describe"   : false,
    "it"         : false,
    "to"         : false,
    "ok"         : false,
    "be"         : false,
    "before"     : false,
    "beforeEach" : false,
    "after"      : false,
    "afterEach"  : false,
    "equal"      : false,
    "angular"    : true,
    "expect"     : true
  },
  "globalstrict": true,
  "quotmark": false,
  "smarttabs": true,
